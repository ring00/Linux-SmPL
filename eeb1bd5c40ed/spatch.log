diff -u -p a/afs/rxrpc.c b/afs/rxrpc.c
--- a/afs/rxrpc.c
+++ b/afs/rxrpc.c
@@ -78,7 +78,8 @@ int afs_open_socket(void)
 		return -ENOMEM;
